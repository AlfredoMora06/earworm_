    <ResponsiveContainer width="100%" height={700}>
      <BarChart
        width={600}
        height={700}
        data={GenreObjectArray}
        layout="vertical"
        margin={{
        top: 5,
        right: 35,
        left: 25,
        bottom: 5
      }}>
            <defs>
        <linearGradient
          id="colorUv"
          x1="1"
          y1="0"
          x2="1"
          y2="100%"
          spreadMethod="reflect"
        >
          <stop offset="0" stopColor="#4B526E" />
          <stop offset="1" stopColor="#8086A0" />
        </linearGradient>
      </defs>
      <Tooltip />
        <XAxis hide= {true} type="number" domain={[0, 'dataMax']}/>
        <YAxis hide= {true} type="category" dataKey="genre" />
        <Bar dataKey="number" radius={5} fill="url(#colorUv)">
        <LabelList dataKey="genre" position="right" fill="#8086A0"  />
        <LabelList dataKey="number" position="left" fill="#8086A0"  />
        </Bar>
      </BarChart>
    </ResponsiveContainer>


    <ResponsiveContainer width="100%" height={300}>
        <PieChart height={300}>
          <Pie data={mostOccuringArtists} //paddingAngle={5
         dataKey="count" nameKey="artist" cx="50%" cy="50%" label //innerRadius={35
         outerRadius={100}>
            {/* we map over the array to create Cell components -- these allow us to cutom color segments of chart */}
            {mostOccuringArtists.map((entry, index) => (
              <Cell
                key={`cell-${index}`}
                cornerRadius={2}
                stroke="#202743"
                strokeWidth={0.5}
                fill={colors[index]}></Cell>
            ))}
          </Pie>
          <Legend
            wrapperStyle={{
            color: "#FFFFFF",
            fontSize: "13px",
            marginRight: "5%"
          }}
            layout="vertical"
            verticalAlign="middle"
            iconType="circle"
            align="right"/>
        </PieChart>
      </ResponsiveContainer>